==> xa_leakage_opts.sp <==
.option XA_CMD="set_array_option -rc_optimize 0 "
.option XA_CMD="set_probe_option -dump_waveform_file 0 "
.option XA_CMD="set_duplicate_rule -select_model last "
.option XA_CMD="set_duplicate_rule -select_subckt last "
.option XA_CMD="set_duplicate_rule -subckt_port on "
.option XA_CMD="set_probe_option -skip_flat_pl_node 1 "
.option XA_CMD="set_wildcard_rule -node_alias 1 "
.option XA_CMD="set_analysis_core -core 0 "
.option XA_CMD="set_multi_core -core 1 "
.option XA_CMD="set_message_option -pattern "Floating (no DC path to gnd) bulk" -action exit"

==> xa_power_opts.sp <==
.option XA_CMD="set_probe_option -dump_waveform_file 0 "
.option XA_CMD="set_duplicate_rule -select_model last "
.option XA_CMD="set_duplicate_rule -select_subckt last "
.option XA_CMD="set_duplicate_rule -subckt_port on "
.option XA_CMD="set_probe_option -skip_flat_pl_node 1 "
.option XA_CMD="set_wildcard_rule -node_alias 1 "
.option XA_CMD="set_analysis_core -core 0 "
.option XA_CMD="set_multi_core -core 1 "
.option XA_CMD="set_message_option -pattern "Floating (no DC path to gnd) bulk" -action exit "
.option XA_CMD="set_dc_option -method adaptive spice "

==> xa_timing_accurate_opts.sp <==
.option XA_CMD="set_resistor_option	-rule	2	-min	0	-max	1000000000"
.option XA_CMD="set_duplicate_rule	-select_model	last"
.option XA_CMD="set_duplicate_rule	-subckt_port	on"
.option XA_CMD="set_probe_option	-skip_flat_pl_node	1"
.option XA_CMD="set_wildcard_rule	-node_alias	1"
.option XA_CMD="set_tolerance_option	-tol	25"
.option XA_CMD="set_model_option	-grid	0.01"
.option XA_CMD="set_array_option	-rc_optimize	0"
.option XA_CMD="set_message_option -pattern "Floating (no DC path to gnd) bulk" -action exit"
.option XA_CMD="set_dc_option -method adaptive spice"

==> xa_timing_opts.sp <==
.option XA_CMD="set_duplicate_rule -select_model last "
.option XA_CMD="set_duplicate_rule -select_subckt last "
.option XA_CMD="set_duplicate_rule -subckt_port on "
.option XA_CMD="set_probe_option -skip_flat_pl_node 1 "
.option XA_CMD="set_wildcard_rule -node_alias 1 "
.option XA_CMD="set_analysis_core -core 0 "
.option XA_CMD="set_multi_core -core 1 "
.option XA_CMD="set_message_option -pattern "Floating (no DC path to gnd) bulk" -action exit"
.option XA_CMD="report_floating_node -type all -format report"
.option XA_CMD="set_dc_option -method adaptive spice"
